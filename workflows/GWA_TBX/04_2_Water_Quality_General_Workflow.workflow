.NAME:01_Manual_Workflow-02_Water Quality Assessment
.GROUP:PG#4:Water Quality
.ALGORITHM:beam:fub.water
.PARAMETERS:{"computeCHL": true, "checkWhetherSuspectIsValid": true, "computeAtmCorr": true, "computeYS": true, "computeTSM": true, "expression": "(radiance_13- radiance_8)/(radiance_8+ radiance_13) < 0.05 and radiance_13 < 50 and not l1p_flags.CC_CLOUD_SHADOW and not l1p_flags.CC_CLOUD and not l1p_flags.CC_CLOUD_BUFFER"}
.MODE:Normal
.INSTRUCTIONS:<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN" "http://www.w3.org/TR/REC-html40/strict.dtd">
<html><head><meta name="qrichtext" content="1" /><style type="text/css">
